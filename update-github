# ! /bin/bash
repo="origin"
branchName="melbourne"

echo "====================================="
echo "           GITHUB UPDATE              "
echo "====================================="

git add . 

git commit -m"Update"

git push "$repo" "$branchName"

echo "====================================="
echo "           FINISHED!                 "
echo "====================================="
echo "PUSH BRANCH: $branchName TO $repo"